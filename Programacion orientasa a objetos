        "class Alumni(Alumno):\n",
        "  def __init__(self, nombre, apellido, edad, colegio, año, empleo, egreso):\n",
        "    super().__init__( nombre, apellido, edad, colegio, año)\n",
        "    self.empleado= empleo \n",
        "    self.añoegreso= egreso\n",
        "  def get_egreso(self):\n",
        "    return self.añoegreso\n",
        "  def __str__(self):\n",
        "    return(\"Bienvenido al portal \"+self.nombre +\" \"+ self.apellido+ \" de \" +self.edad\n",
        "+ \" años, sus caracteristicas importantes son: Egresaste del colegio \"+self.colegioegreso \n",
        "+\" el año \"+str(self.añoingreso) +\", luego, entraste a esta universidad, en la cual te graduaste el año \"+str(self.añoegreso)  \n",
        "+ \" y ahora te encuentras \"+self.empleado)"
