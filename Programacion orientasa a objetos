        "class Rector:\n",
        "  def __init__(self, nombre, apellido, edad):\n",
        "    self.nombre = nombre\n",
        "    self.apellido = apellido\n",
        "    self.edad = edad\n",
        "  def __str__(self):\n",
        "    return(\"Saludos cordiales, soy \"+ self.nombre+ \" \" +self.apellido+\" el rector de la escuela y tengo \" +str(self.edad) +\" años.\")\n",
        "class Alumno(Rector):\n",
        "  def __init__(self, nombre, apellido, edad, colegio, año):\n",
        "    super().__init__(nombre, apellido, edad)\n",
        "    self.colegioegreso = colegio\n",
        "    self.añoingreso = año\n",
        "  def set_nombre(self,nuevo_nombre):\n",
        "    self.nombre = nuevo_nombre\n",
        "  def get_nombre(self):\n",
        "    return self.nombre\n",
        "  def __str__(self):\n",
        "    return(\"Tu eres \" +self.nombre + \" \"+ self.apellido+ \" tienes \" +str(self.edad)\n",
        "  + \" saliste de la insitucion \" +self.colegioegreso+ \" e ingresaste a la carrera en el año \" +str(self.añoingreso)+\".\")\n",
        "class Profesor(Rector):\n",
        "  def __init__(self, nombre, apellido, edad, añosestudio, institucionegreso):\n",
        "    super().__init__(nombre, apellido, edad)\n",
        "    self.año= añosestudio\n",
        "    self.insti= institucionegreso\n",
        "  def get_edad(self):\n",
        "    return self.edad\n",
        "  def set_edad(self, nueva_edad):\n",
        "    self.nombre = nueva_edad\n",
        "  def __str__(self):\n",
        "    return(\"Bienvenido estimado \" +self.nombre + \" \"+ self.apellido+ \" de\" +str(self.edad)+ \"de edad, \" + \n",
        "\"docente en el area de Telecomunicaciones, el cual estudio durante \"+str(self.año) +\" en la prestigiosa universidad \"+ self.insti ) \n",
        "class auxiliar(Rector):\n",
        "  def __init__(self, nombre, apellido, edad, funcion, añosexperiencia):\n",
        "    super().__init__(nombre, apellido, edad)\n",
        "    self.funcion = funcion\n",
        "    self.exp = añosexperiencia\n",
        "  def get_edad(self):\n",
        "    return self.edad\n",
        "  def __str__(self):\n",
        "    return(\"Tu eres \" +self.nombre + \" \"+ self.apellido+ \" tienes \" +str(self.edad)+ \" , tu funcion es  \" +self.funcion \n",
        "  + \" y tienes \" +str(self.exp)+\" años de experiencia.\")"
      ],
